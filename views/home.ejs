<%- include("partials/header"); -%>


<h1>Home</h1>

<p> <%= startingContent %> </p>

<div>
  <% posts.forEach(function(element){ %>
    <h1> <%= element.title %> </h1>
    <% if (element.content.length > 100) { %>
    <p>
      <%= element.content.substring(0, 100) %>...
      <a href="posts/<%= element.title %>""> Read More </a>
    </p>
    <% } else { %>
      <p> <%= element.content %></p>
    <% } %>
  <% }) %>
</div>

<%- include partials/footer.ejs %>
